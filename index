        // Secciones por semestre
        const seccionesPorSemestre = {
            '5TO': ['J'],
            '6TO': ['I'],
            '8VO': ['I', 'J']
        };

        // Referencias a elementos
        const semestreSelect = document.getElementById('semestre');
        const seccionSelect = document.getElementById('seccion');
        const cedulaInput = document.getElementById('cedula');
        const btnBuscar = document.getElementById('btnBuscar');
        const resultadoDiv = document.getElementById('resultado');

        // Event listeners
        semestreSelect.addEventListener('change', actualizarSecciones);
        btnBuscar.addEventListener('click', buscarNotas);
        
        // Tambi칠n buscar al presionar Enter en el campo de c칠dula
        cedulaInput.addEventListener('keypress', function(e) {
            if (e.key === 'Enter') {
                buscarNotas();
            }
        });

        function actualizarSecciones() {
            const semestre = semestreSelect.value;
            seccionSelect.innerHTML = '';
            
            if (!semestre) {
                seccionSelect.innerHTML = '<option value="">-- Primero selecciona el semestre --</option>';
                seccionSelect.disabled = true;
                return;
            }
            
            const secciones = seccionesPorSemestre[semestre] || [];
            
            if (secciones.length === 1) {
                // Si solo hay una secci칩n, seleccionarla autom치ticamente
                seccionSelect.innerHTML = '<option value="' + secciones[0] + '">Secci칩n ' + secciones[0] + '</option>';
            } else {
                seccionSelect.innerHTML = '<option value="">-- Selecciona la secci칩n --</option>';
                secciones.forEach(function(sec) {
                    const option = document.createElement('option');
                    option.value = sec;
                    option.textContent = 'Secci칩n ' + sec;
                    seccionSelect.appendChild(option);
                });
            }
            
            seccionSelect.disabled = false;
        }

        function buscarNotas() {
            const semestre = semestreSelect.value;
            const seccion = seccionSelect.value;
            const cedula = cedulaInput.value.trim().replace(/\D/g, '');
            
            // Validaciones
            if (!semestre) {
                mostrarError('Por favor, selecciona el semestre.');
                return;
            }
            
            if (!seccion) {
                mostrarError('Por favor, selecciona la secci칩n.');
                return;
            }
            
            if (!cedula) {
                mostrarError('Por favor, ingresa tu n칰mero de c칠dula.');
                return;
            }
            
            // Buscar estudiante
            const estudiante = estudiantes.find(function(e) {
                return e.cedula === cedula && 
                       e.semestre === semestre && 
                       e.seccion === seccion;
            });
            
            if (!estudiante) {
                mostrarError('No se encontr칩 un estudiante con la c칠dula ' + cedula + ' en ' + semestre + ' ' + seccion + '. Verifica los datos ingresados.');
                return;
            }
            
            mostrarResultado(estudiante);
        }

        function mostrarError(mensaje) {
            resultadoDiv.innerHTML = 
                '<div class="error-message">' +
                    '<div class="icon">游땟</div>' +
                    '<h3>No encontrado</h3>' +
                    '<p>' + mensaje + '</p>' +
                '</div>';
            resultadoDiv.classList.add('show');
        }

        function mostrarResultado(est) {
            const inicial = est.nombre.charAt(0).toUpperCase();
            const aprobado = est.nota_final >= 10;
            const statusClass = aprobado ? 'aprobado' : 'reprobado';
            const statusText = aprobado ? 'APROBADO' : 'REPROBADO';
            const gradeClass = aprobado ? '' : 'reprobado';
            
            // Generar mensaje de felicitaci칩n
            let mensajeHTML = '';
            if (est.nota_final >= 18 && est.nota_final <= 20) {
                mensajeHTML = 
                    '<div class="message-box excellent">' +
                        '<div class="emoji">游끥游</div>' +
                        '<div class="title">춰Excelente Desempe침o!</div>' +
                        '<div class="text">춰Felicitaciones! Has demostrado un nivel de excelencia acad칠mica sobresaliente. Tu dedicaci칩n y esfuerzo son un ejemplo a seguir. 춰Sigue as칤!</div>' +
                    '</div>';
            } else if (est.nota_final >= 15 && est.nota_final <= 17) {
                mensajeHTML = 
                    '<div class="message-box good">' +
                        '<div class="emoji">游녪游눩</div>' +
                        '<div class="title">춰Buen Trabajo!</div>' +
                        '<div class="text">Has logrado un buen rendimiento acad칠mico. Con un poco m치s de esfuerzo podr치s alcanzar la excelencia. 춰T칰 puedes llegar m치s alto!</div>' +
                    '</div>';
            }
            
            resultadoDiv.innerHTML = 
                '<div class="student-card">' +
                    '<div class="student-header">' +
                        '<div class="student-avatar">' + inicial + '</div>' +
                        '<div class="student-info">' +
                            '<h3>' + est.nombre + '</h3>' +
                            '<p>C.I.: ' + est.cedula + ' | ' + est.semestre_seccion + '</p>' +
                        '</div>' +
                    '</div>' +
                    
                    '<div class="course-name">' +
                        '<span>游닀 ' + est.unidad + '</span>' +
                    '</div>' +
                    
                    '<div class="grades-grid">' +
                        '<div class="grade-card">' +
                            '<div class="label">1er Corte</div>' +
                            '<div class="value">' + est.corte1 + '</div>' +
                            '<div class="max">/ 25 pts</div>' +
                        '</div>' +
                        '<div class="grade-card">' +
                            '<div class="label">2do Corte</div>' +
                            '<div class="value">' + est.corte2 + '</div>' +
                            '<div class="max">/ 25 pts</div>' +
                        '</div>' +
                        '<div class="grade-card">' +
                            '<div class="label">3er Corte</div>' +
                            '<div class="value">' + est.corte3 + '</div>' +
                            '<div class="max">/ 25 pts</div>' +
                        '</div>' +
                        '<div class="grade-card">' +
                            '<div class="label">4to Corte</div>' +
                            '<div class="value">' + est.corte4 + '</div>' +
                            '<div class="max">/ 25 pts</div>' +
                        '</div>' +
                    '</div>' +
                    
                    '<div class="final-grade-section ' + gradeClass + '">' +
                        '<div class="final-grade-label">Calificaci칩n Final</div>' +
                        '<div class="final-grade-value">' + est.nota_final + '</div>' +
                        '<span class="status-badge ' + statusClass + '">' + statusText + '</span>' +
                    '</div>' +
                    
                    '<div class="progress-section">' +
                        '<div class="progress-header">' +
                            '<span>Puntos acumulados</span>' +
                            '<span>' + est.acumulado + ' / 100 pts</span>' +
                        '</div>' +
                        '<div class="progress-bar">' +
                            '<div class="progress-fill" style="width: ' + est.acumulado + '%"></div>' +
                        '</div>' +
                    '</div>' +
                    
                    mensajeHTML +
                '</div>';
            
            resultadoDiv.classList.add('show');
            
            // Scroll suave al resultado
            setTimeout(function() {
                resultadoDiv.scrollIntoView({ behavior: 'smooth', block: 'start' });
            }, 100);
        }
    </script>
</body>
</html>
